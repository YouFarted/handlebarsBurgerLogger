<h1>Eat-Da-Burger</h1>

<div>Uneaten</div>
<div id="uneaten-parent">
  {{#each burgers}}
    {{#unless eaten}}
  	<div>{{this.id}}: {{this.name}}</div> <button data-id="{{this.id}}">Eat it!</button>
    {{/unless}}
  {{/each}}
</div>

<hr/><hr/><hr/>

<div>Already Eaten</div>
<div id="eaten-parent">
  {{#each burgers}}
    {{#if eaten}}
    <div style="text-decoration: line-through;">{{this.id}}: {{this.name}}</div>
    <button data-id="{{this.id}}">uneat it!</button>
    {{/if}}
  {{/each}}
</div>
